import java.util.ArrayList;


public class tweetData {
    private double completed_in;
    private long max_id;
    private long max_id_str;
    private String next_page;
    private int page;
    private String query;
    private String refresh_url;
    private ArrayList<tweetDetails> tweets = new ArrayList<tweetDetails>();
    
    public String getNextPage() {
    	return next_page;
    }

	public ArrayList<tweetDetails> getTweets() {
		return tweets;
	}

	public void setTweets(ArrayList<tweetDetails> tweets) {
		this.tweets = tweets;
	}

	public String getRefresh_url() {
		return refresh_url;
	}

	public void setRefresh_url(String refresh_url) {
		this.refresh_url = refresh_url;
	}

	public String getQuery() {
		return query;
	}

	public void setQuery(String query) {
		this.query = query;
	}

	public int getPage() {
		return page;
	}

	public void setPage(int page) {
		this.page = page;
	}
}
