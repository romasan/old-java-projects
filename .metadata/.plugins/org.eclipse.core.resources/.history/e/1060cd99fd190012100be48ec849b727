
public class figure implements shape {

		protected int x;
		protected int y;
		@Override
		public void setCoord(int x, int y) {
			this.x = x;
			this.y = y;
		}
		public int getX() {
			return this.x;
		}
		public int getY() {
			return this.y;
		}
		@Override
		public void draw() {
			
		

		}

		
	}

