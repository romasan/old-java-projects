
public class rectangle extends shape {
	protected int width;
	protected int height;
	
	public void setSize(int width, int height) {
		this.width = width;
		this.height = height;
	}

	public void draw() {
		System.out.println("rectangle ok x: " + super.crd.x + " y: " + super.crd.y + " width: " + this.width + " height: " + this.height);
	}
}
