

public class rectangle extends figure {
	protected int Height;
	protected int Width;
	
	public void setRadius(int Height, int Width) {
		this.Height = Height;
		this.Width = Width;
		}
	
	public void draw() {	
		System.out.printf("rectangledraw  x: " + super.x + " y: " + super.y );
	}

	}

