
public class shape implements ishape {
protected coord crd = new coord();
@Override
public void setCoord(int x, int y) {
	this.crd.x = x;
	this.crd.y = y;
}
@Override
public coord getCoord() {
	return this.crd;
}
@Override
public void draw() {
	//System.out.println("ok x: " + this.x + " y: " + this.y);
}

}
